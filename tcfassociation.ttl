@base <http://tcftest.org/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix tcf-pouch: <http://tcftest.org/pouch/> .

<> a owl:Ontology ;
	dc:title "The TCF Logistics open ontology" ;
	dc:description "The TCF Logistics Open Ontology is a OWL ontology designed to support the logistics industry." .

<LogisticsObject> a owl:Class ;
	rdfs:label "Logistics Object" ;
	rdfs:comment "Hasse changed again, Need a good defintion. Here is a place holder - A general classifcation of all objects that are used in the logistics process.  A logistics object might be a plane, a truck. It might also be a document, or a package or a consignment." .
	
<ShippersInstruction> a owl:Class ;
	rdfs:label "Shippers Instruction" ;
	rdfs:comment "Need a good defintion. Here is one from shippingsolutions.com - The Shipper's Letter of Instruction helps to convey specific instructions from the exporter to his agent, usually an international freight forwarder." ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .
	
<Consignment> a owl:Class ;
	rdfs:label "Consignment" ;
	rdfs:comment "A separately identifiable collection of goods items to be transported or available to be transported from one consignor to one consignee via one or more modes of transport where each consignment is the subject of one single transport contract." ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .

<Package> a owl:Class ;
	rdfs:label "Package" ;
	rdfs:comment "A physical package for this trade line item." ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .

<CommercialInvoice> a owl:Class ;
	rdfs:label "Commercial Invoice" ;
	rdfs:comment "Need a good defintion of a package. Here is one from wikipedia.org - When used in foreign trade, a commercial invoice is a customs document. It is used as a customs declaration provided by the person or corporation that is exporting an item across international borders." ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .

<DangerousGoodsDeclaration> a owl:Class ;
	rdfs:label "Dangerous Goods Declaration" ;
	rdfs:comment "Need a good defintion of a package. Here is one from businessdictionary.com - Document(s) prepared by a consignor or shipper to certify that the dangerous goods being transported have been packaged, labeled, and declared in accordance with the standard international shipping regulations." ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .

<LogisticsVehicle> a owl:Class ;
	rdfs:label "Logistics Vehicle" ;
	rdfs:comment "Need a good defintion of a logistics vehicle. Here is one from businessdictionary.com - Document(s) prepared by a consignor or shipper to certify that the dangerous goods being transported have been packaged, labeled, and declared in accordance with the standard international shipping regulations." ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .

<HAWB> a owl:Class ;
	rdfs:label "House AirWayBill" ;
	rdfs:comment "TBD" ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2018-02-13"^^<http://www.w3.org/2001/XMLSchema#date> .

<MAWB> a owl:Class ;
	rdfs:label "Master AirWayBill" ;
	rdfs:comment "TBD" ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2018-02-13"^^<http://www.w3.org/2001/XMLSchema#date> .

<FlightManifest> a owl:Class ;
	rdfs:label "Flight Manifest" ;
	rdfs:comment "TBD" ;
	rdfs:subClassOf <LogisticsObject> ;
	dc:modified "2018-02-13"^^<http://www.w3.org/2001/XMLSchema#date> .

<IOLIdentifier> a owl:Class ;
	rdfs:label "IOL Identifier" ;
	rdfs:comment "The Internet of Logistics Identifier for a connected company." ;
	dc:modified "2017-11-16"^^<http://www.w3.org/2001/XMLSchema#date> .
	
<forwarder> a owl:ObjectProperty ;
	rdfs:domain <ShippersInstruction> ;
	rdfs:range schema:Organization ;
	rdfs:label "forwarder" ;
	rdfs:description "The party undertaking the freight forwarding of these consignments." .

<consignment> a owl:ObjectProperty ;
	rdfs:domain <ShippersInstruction> ;
	rdfs:range <Consignment> ;
	rdfs:label "Consignment" ;
	rdfs:description "A separately identifiable collection of goods items to be transported or available to be transported from one consignor to one consignee via one or more modes of transport where each consignment is the subject of one single transport contract." .

<shippersInstructionShippersReference> a owl:ObjectProperty ;
	rdfs:domain <ShippersInstruction> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "Reference" ;
	rdfs:description "The reference for the shippers instruction in the shippers system" .

<quantityOfPackages> a owl:ObjectProperty ;
	rdfs:domain <ShippersInstruction> ;
	rdfs:range xsd:int ;
	rdfs:label "Quantity of packages" ;
	rdfs:description "The number of packages in the complete shippers instruction" .

<volume> a owl:ObjectProperty ;
	rdfs:domain <ShippersInstruction> ;
	rdfs:range xsd:double ;
	rdfs:label "Volume" ;
	rdfs:description "The volume related to the shippers instruction" .

<GrossWeigth> a owl:ObjectProperty ;
	rdfs:domain <ShippersInstruction> ;
	rdfs:range xsd:double ;
	rdfs:label "GrossWeigth" ;
	rdfs:description "A measure of the gross weight (mass) of these consignments which includes the weight of packaging but which excludes the weight of any transport equipment." .

# Start of consignment
<Consignment> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range rdfs:literal ;
	rdfs:label "Consignment" ;
	rdfs:description "A separately identifiable collection of goods items to be transported or available to be transported from one consignor to one consignee via one or more modes of transport where each consignment is the subject of one single transport contract." .

<Consignor> a owl:objectproperty;
	rdfs:domain <Consignment>;
	rdfs:range schema:Organization;
	rdfs:label "Consignor";
	rdfs:description "The consignor party for this consignment.".

<Consignee> a owl:objectproperty;
	rdfs:domain <Consignment>;
	rdfs:range schema:Organization;
	rdfs:label "Consignee";
	rdfs:description "The consignee party for this consignment.".

<Buyer> a owl:objectproperty;
	rdfs:domain <Consignment>;
	rdfs:range schema:Organization;
	rdfs:label "Buyer";
	rdfs:description "The buyer party for this consignment.".

<Seller> a owl:objectproperty;
	rdfs:domain <Consignment>;
	rdfs:range schema:Organization;
	rdfs:label "Buyer";
	rdfs:description "The Seller party for this consignment.".

<Package> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "Package" ;
	rdfs:description "A physical package for this trade line item." .

<Incoterm> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "Incoterm" ;
	rdfs:description "A code specifying a type of delivery for these delivery terms such as the INCOTERMS Terms of Delivery." .

<IncotermsLocation> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "IncotermsLocation" ;
	rdfs:description "A location relevant for these delivery terms." .

<RequestedDeliveryDate> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range xsd:date ;
	rdfs:label "RequestedDeliveryDate" ;
	rdfs:description "Ultimate Ship To Delivery Date Time." .

<TransportAgreementLaneID> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "TransportAgreementLaneID" ;
	rdfs:description "TBD." .

<RequestedPickupDate> a owl:ObjectProperty ;
	rdfs:domain <Consignment> ;
	rdfs:range xsd:date ;
	rdfs:label "RequestedPickupDate" ;
	rdfs:description "A date, time, date time, or other date time value when this trade delivery is available for pick-up." .


# Start of Package
<PackageReference> a owl:ObjectProperty ;
	rdfs:domain <Package> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "PackageReference" ;
	rdfs:description "A packaging identifier, expressed as text, for this trade line item" .

<PackageType> a owl:ObjectProperty ;
	rdfs:domain <Package> ;
	rdfs:range rdfs:Literal ;
	rdfs:label "PackageType" ;
	rdfs:description "A packaging type, expressed as text, for this trade line item" .

<volume> a owl:ObjectProperty ;
	rdfs:domain <Package> ;
	rdfs:range xsd:double ;
	rdfs:label "Volume" ;
	rdfs:description "A measure of the gross volume of this trade line item." .

<Length> a owl:ObjectProperty ;
