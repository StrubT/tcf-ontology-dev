	rdfs:range xsd:double ;
	rdfs:label "Length" ;
	rdfs:description "A measure of the length of this trade line item." .

<Width> a owl:ObjectProperty ;
	rdfs:domain <Package> ;
	rdfs:range xsd:double ;
	rdfs:label "Width" ;
